{
  "name": "nekoro",
  "version": "0.0.0",
  "license": "Apache-2",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "rimraf": "rimraf",
    "start": "ng serve",
    "prebuild": "run-s \"rimraf build\" \"coverage\"",
    "build": "ng build -t production --aot",
    "checkdep": "nsp check",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "pretest": "run-s \"checkdep\" \"lint\"",
    "test": "ng test",
    "precoverage": "rimraf coverage",
    "coverage": "npm run test -- -cc -sr",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "test:all": "run-s \"coverage\" \"e2e\"",
    "clean:all": "run-s \"rimraf coverage\" \"rimraf dist\"",
    "coveralls": "cat ./coverage/coverage.lcov | coveralls"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/db": "^2.0.1",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/notify": "^1.0.0",
    "@ngrx/router-store": "^1.2.5",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.2",
    "core-js": "^2.4.1",
    "debug": "^2.6.0",
    "immutable": "^3.8.1",
    "reselect": "^2.5.4",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/debug": "0.0.29",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "@types/reselect": "^2.0.27",
    "angular-cli": "1.0.0-beta.25.5",
    "codelyzer": "~2.0.0-beta.1",
    "coveralls": "^2.11.15",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "nsp": "^2.6.2",
    "protractor": "~4.0.13",
    "rimraf": "^2.5.4",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  }
}
